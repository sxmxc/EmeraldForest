PlayerInventory="*res://Inventory/Scripts/PlayerInventory.gd"

[display]

window/stretch/mode="viewport"
window/size/width=1366
window/size/height=768
window/size/test_width=1366.0
window/size/test_height=768.0

[editor_plugins]

enabled=PackedStringArray()

[gdnative]

singletons=[]

