[application]

config/name="SmokeyDew"
run/main_scene="res://Scenes/Menus/StartScreen.tscn"
boot_splash/image="res://Leaf-Icon.png"
boot_splash/use_filter=false
boot_splash/bg_color=Color(0.129412, 0.00392157, 0.298039, 1)
config/icon="res://Leaf-Icon.png"
config/windows_native_icon="res://smokeydew.ico"
run/fullsize=true
run/use_filter=true
run/bg_color=Color(0.14, 0.14, 0.14, 1)
run/image=""
config/features=PackedStringArray("4.0")

[autoload]

ggsManager="*res://addons/GGS/src/ggs_manager.gd"
SceneManager="*res://addons/scene_manager/SceneManager.tscn"
Global="*res://Core/Global.gd"
GameClock="*res://Core/GameClock.gd"
Console="*res://addons/quentincaffeino/console/src/Console.tscn"
